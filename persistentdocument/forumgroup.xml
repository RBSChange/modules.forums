    <property name="topic" type="modules_website/systemtopic" min-occurs="1" inverse="true" cascade-delete="true"/>
    <property name="website" type="modules_website/website" min-occurs="1" inverse="true"/>
    <property name="metatitle" type="LongString" localized="true"/>
    <property name="metadescription" type="LongString" localized="true"/>
    <property name="metakeywords" type="LongString" localized="true"/>
    <property name="lockedMode" type="Integer" default-value="0" from-list="modules_forums/modeslimited"/>
    <property name="locked" type="Boolean" default-value="false"/>
    <property name="excludeFromRssMode" type="Integer" default-value="0" from-list="modules_forums/modeslimited"/>
    <property name="excludeFromRss" type="Boolean"/>
  </properties>
